{
    "name": "ldaliwan/ssa-wordpress-eloquent",
    "description": "Create simple custom eloquent model for ssa wordpress project",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "Ldaliwan\\SsaWordpressEloquent\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev":{
        "psr-4": {
            "Tests\\":"tests/"
        }
    },
    "require": {
        "php": ">=7.2.0"  
      },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "authors": [
        {
            "name": "LouieDaliwan",
            "email": "louie@ssagroup.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true  
}
